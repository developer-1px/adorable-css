    "prettier-plugin-svelte": "^3.3.3",
    "svelte": "^5.25.6",
    "svelte-check": "^4.1.5",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.29.0",
    "unocss": "^66.0.0",
    "vite": "^6.2.4",
    "vitest": "^3.1.1"
  },
  "packageManager": "pnpm@10.7.1"
