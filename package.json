    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.20.3",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@types/clean-css": "^4.2.11",
    "@types/eslint": "^9.6.1",
    "@types/glob": "^8.1.0",
    "cac": "^6.7.14",
    "chevrotain": "^11.0.3",
    "chokidar": "^4.0.3",
    "clean-css": "^5.3.3",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
    "gh-pages": "^6.3.0",
    "glob": "^11.0.1",
    "glob-to-regexp": "^0.4.1",
    "globals": "^16.0.0",
